        "functionCode": "// Code here will run only once, no matter how many input items there are.\n// More info and help: https://docs.n8n.io/nodes/n8n-nodes-base.function\n// Tip: You can use luxon for dates and $jmespath for querying JSON structures\n\n// Loop over inputs and add a new field called 'myNewField' to the JSON of each one\nfunction getSelectedTexts(questions, label) {\n  const question = questions.find(q => q.label === label);\n  if (!question) return null;\n\n  return question.value? question.value.map(\n    id => question.options.find(opt => opt.id === id)?.text\n  ).filter(Boolean).join(\", \"): \"\"; // remove undefined se algum id não existir\n}\nlet risco = 'nenhum';\nlet priority = 'low';\nfor (item of items) {\n   var form_answers = item.json.body.data.fields;\n  \n // Object.keys(results).forEach(key => {\n  //  if (results[key] === \"\" || results[key] === //null) {\n//      delete results[key];\n     // }\n // });\n  \n\n  var dados_voluntaria = {\n\tnome: form_answers.find(objeto => objeto.label == \"nome_completo\").value,\n    email: form_answers.find(objeto => objeto.label == \"email_de_cadastro\").value,\n  }\n\n  var dados_acolhida = {\n    nome: form_answers.find(objeto => objeto.label == \"nome_da_acolhida\").value,\n    data_de_nascimento: form_answers.find(objeto => objeto.label == \"data_de_nascimento\").value,\n    identidade_racial: getSelectedTexts(form_answers, \"identidade_racial\"),\n    identidade_genero:getSelectedTexts(form_answers, \"identidade_genero\")?.replaceAll(\" (que se identifica com o sexo que lhe foi designado ao nascer)\",\"\").replaceAll(\" (possui outra identidade de gênero, diferente da que lhe foi designada ao nascer)\",\"\"),\n    deficiencia:getSelectedTexts(form_answers, \"deficiencia\"),\n    pcd:getSelectedTexts(form_answers, \"pcd\"),\n    estado_civil:getSelectedTexts(form_answers, \"estado_civil\"),\n    filhos_faixa_etaria:getSelectedTexts(form_answers, \"filhos_faixa_etaria\"),\n    filhos_pcd: getSelectedTexts(form_answers, \"filhos_pcd\"),\n    deficiencia_filhos:getSelectedTexts(form_answers, \"deficiencia_filhos\"),\n    tem_filhos:getSelectedTexts(form_answers, \"tem_filhos\"),\n    gravida_ou_tem_recem_nascido:getSelectedTexts(form_answers, \"gravida_ou_tem_recem_nascido\"),\n  }\n  \n  let data = new Date(dados_acolhida.data_de_nascimento);\n  let data_de_nascimento_br =   ((data.getDate() )) + \"/\" + ((data.getMonth() + 1)) + \"/\" + data.getFullYear();\n  \n  var aspectos_socioeconomicos = {\n    escolaridade: getSelectedTexts(form_answers, \"escolaridade\"),\n  }\n\n  var moradia = {\n    local_de_risco: getSelectedTexts(form_answers, \"local_de_risco\"),\n  }\n\n  var qualidade_de_vida = {\n    impacto_sofrido_moderado: getSelectedTexts(form_answers, \"impacto_sofrido_moderado\"),\n    impacto_sofrido_grave: getSelectedTexts(form_answers, \"impacto_sofrido_grave\")\n  } \n  var historico_violencia = {\n    tipos_violencia: getSelectedTexts(form_answers, \"tipos_violencia\"),\n    violencia_fisica_grave: getSelectedTexts(form_answers, \"violencia_fisica_grave\"),\n    violencia_fisica_extrema: getSelectedTexts(form_answers, \"violencia_fisica_extrema\"),\n    se_sente_isolada: getSelectedTexts(form_answers, \"se_sente_isolada\"),\n    autor_tem_acesso_arma_fogo: getSelectedTexts(form_answers, \"autor_tem_acesso_arma_fogo\"),\n    como_ameacou: getSelectedTexts(form_answers, \"como_ameacou\"), \n    //explique_como_ameacou:  form_answers.find(objeto => objeto.label == \"explique_como_ameacou\").value?form_answers.find(objeto => objeto.label == \"explique_como_ameacou\").value: \"\" ,\n    autor_ameacou_familiar: getSelectedTexts(form_answers,\"autor_ameacou_familiar\"), \n    autor_obrigou_ato_sexual:getSelectedTexts(form_answers, \"autor_obrigou_ato_sexual\"),\n    violencia_durante_gravidez:getSelectedTexts(form_answers, \"violencia_durante_gravidez\"),\n    autor_comportamentos_abusivos:getSelectedTexts(form_answers, \"autor_comportamentos_a