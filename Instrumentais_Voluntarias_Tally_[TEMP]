        "functionCode": "let risco = 'nenhum';\nlet priority = 'low';\n\nfunction getSelectedTexts(questions, label) {\n  const question = questions.find(q => q.label === label);\n  if (!question) return [];\n\n  return question.value? question.value.map(\n    id => question.options.find(opt => opt.id === id)?.text\n  ).filter(Boolean).join(\", \"): \"\"; // remove undefined se algum id não existir\n}\nfunction filterLevel(input) {\n  const permitidas = [\"média\", \"ruim\", \"boa\"];\n  \n  return input\n    .split(/\\s+/) // quebra a string em palavras\n    .filter(palavra => permitidas.includes(palavra.toLowerCase())) // mantém só as permitidas\n    .join(); // junta de volta em uma string\n}\n\nfor (item of items) {\n\n    var form_answers = item.json.body.data.fields;\n\n\tvar dados_voluntaria = {\n    \tnome: form_answers.find(objeto => objeto.label == \"nome_completo\").value,\n      email_cadastro: form_answers.find(objeto => objeto.label == \"email_de_cadastro\").value,\n    }\n\tvar dados_acolhida = {\n\t\tnome: form_answers.find(objeto => objeto.label == \"nome_da_acolhida\").value //\"Acholhida teste\",\n\t}\n\t\n\tvar contrato = {\n\t\ttem_assiduidade_atendimentos: getSelectedTexts(form_answers, \"tem_assiduidade_atendimentos\"), //\"Não (Por favor, entre em contato imediatamente com a equipe)\",\n\t\tmotivo_nao_assiduidade: getSelectedTexts(form_answers, \"motivo_nao_assiduidade\"), //\"Falta sem avisar, O autor da violência impediu\",\n\t\trespeita_combinados_atendimento: getSelectedTexts(form_answers, \"respeita_combinados_atendimento\"), //\"Não\",\n\t\tfrequencia_atendimento_fora_do_horario: getSelectedTexts(form_answers, \"frequencia_atendimento_fora_do_horario\") //\"Três ou mais vezes na semana\",\n\t}\n\n\tvar acompanhamento_psicologico = {\n\t\tfoi_submetida_novas_violencias: getSelectedTexts(form_answers, \"foi_submetida_novas_violencias\"), //\"Física\",\n\t\tcomentario_sobre_novas_violencias: form_answers.find(objeto => objeto.label == \"co