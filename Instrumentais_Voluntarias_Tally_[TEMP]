ctedTexts(form_answers, \"qual_o_regime_de_trabalho_da_acolhida\"),\n    renda_individual: getSelectedTexts(form_answers, \"renda_individual_acolhida\"),\n    tem_independencia_financeira: getSelectedTexts(form_answers, \"acolhida_tem_independencia_financeira\"),\n    tem_cadunico: getSelectedTexts(form_answers,\"acolhida_tem_cadunico\"),\n    recebe_algum_beneficio: getSelectedTexts(form_answers, \"acolhida_tem_benefico\"),\n    responsavel_financeiramente: getSelectedTexts(form_answers, \"acolhida_e_responsavel_financeira\"),\n    divide_responsabilidade_com: getSelectedTexts(form_answers, \"acolhide_divide_responsabilidade_com\"),\n    tem_dependentes_financeiros: getSelectedTexts(form_answers, \"acolhida_tem_dependentes_finaceiros\"),\n    quantidade_de_dependentes: getSelectedTexts(form_answers, \"quantidade_de_dependentes\") === \"\"? \"0\": getSelectedTexts(form_answers, \"quantidade_de_dependentes\").replaceAll(\"+\",\"\"),\n    \n  }\n\n  var moradia = {\n\n    situacao_moradia: getSelectedTexts(form_answers, \"situacao_moradia\"),\n    cedida_por: getSelectedTexts(form_answers, \"cedida_por\"),\n    reside_com_o_autor_da_viloencia: getSelectedTexts(form_answers,\"reside_com_o_autor_da_viloencia\"),\n    sente_seguram_em_casa: getSelectedTexts(form_answers, \"sente_seguram_em_casa\"),\n    situacao_de_carcere: getSelectedTexts(form_answers, \"situacao_de_carcere\")\n  } \n\n  //Determina risco do caso\n  var risco = 'nenhum'\n  var priority = 'low'\n  if(moradia.situacao_de_carcere === 'Sim') {\n    priority = 'urgent'  \n    risco = 'extremo'\n  }\n\n  var servico_publico = {\n    \n    conhece_redes_de_atendimento: getSelectedTexts(form_answers, \"conhece_redes_de_atendimento\"),\n    quais_redes_de_atendimento_conhece: getSelectedTexts(form_answers, \"quais_redes_de_atendimento_conheceu\"),\n    atentendida_em_servico_publico: getSelectedTexts(form_answers, \"atentendida_em_servico_publico\"),\n    quais_servicos_publicos_atentida: getSelectedTexts(form_